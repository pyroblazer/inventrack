{
  "name": "@microservice/api-gateway",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "build:backend": "nest build",
    "clean:microservices": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:backend": "node dist/main",
    "dev": "nest start --watch",
    "clean": "rimraf dist node_modules .turbo",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "type-check": "tsc --noEmit",
    "test:a": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@microservices/common": "workspace:*",
    "@microservices/database": "workspace:*",
    "@microservices/proto": "workspace:*",
    "@microservices/redis": "workspace:*",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/microservices": "^11.1.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/schedule": "^5.0.1",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@shared/types": "workspace:*",
    "@types/express": "^5.0.2",
    "@upstash/redis": "^1.34.9",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "cache-manager": "^6.4.3",
    "chalk": "^4.1.2",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "geoip-lite": "^1.4.10",
    "helmet": "^8.1.0",
    "http-proxy": "^1.18.1",
    "joi": "^17.13.3",
    "nestjs-pino": "^4.4.0",
    "pino-http": "^10.4.0",
    "pino-pretty": "^13.0.0",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.2",
    "useragent": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.2",
    "@shared/eslint-config": "workspace:*",
    "@shared/typescript-config": "workspace:*",
    "@types/compression": "^1.8.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/geoip-lite": "^1.4.4",
    "@types/http-proxy": "^1.17.16",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@types/request-ip": "^0.0.41",
    "@types/supertest": "^6.0.3",
    "@types/useragent": "^2.3.4",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
